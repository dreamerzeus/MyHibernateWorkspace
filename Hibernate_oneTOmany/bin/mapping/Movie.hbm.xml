<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="beans.Movie" table="Movie" schema="becm8">
    <id name="mid"/>
    <property name="moviename"/>
    <many-to-one name="beans.Movie" class="beans.Actor" column="aid"></many-to-one>
    </class>
    </hibernate-mapping>